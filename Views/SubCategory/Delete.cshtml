@model WebMVC.Models.SubCategory


@{
    ViewData["Title"] = "Delete";
}

<br />
<h2>Delete Sub Category</h2>
<br />





            <form method="post" asp-action="Delete">


            <input type="hidden" asp-for="Id" />

            <div>

                <div class="form-group row">
                    <div class="col-md-2">
                        <label asp-for="Category.Name" class="custom-label"></label>
                    </div>
                    <div class="col-md-5">
                        <input asp-for="Category.Name" readonly class="form-control" />
                    </div>
                </div>

                <div class="form-group row">
                    <div class="col-md-2">
                        <label asp-for="Name" class="control-label"></label>
                    </div>
                    <div class="col-md-5">
                        <input asp-for="Name" readonly class="form-control" />
                    </div>

                </div>


                <div class="form-group row">
                    <div class="col-md-5 offset-md-2">
                        <partial name="_DeleteAndBackToListButton" model="Model.Id" />
                    </div>
                </div>
            </div>
            </form>

